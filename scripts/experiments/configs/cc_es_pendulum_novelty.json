{
    "environment": {
        "name": "InvertedPendulum-v2",
        "max_episode_steps": 5000,
        "early_stop_reward": 5000,
        "stochatic": false
    },
    "model": {
      "hidden_size": 64
    },
    "ES": {
      "lr": 1e-2,
      "sigma": 1e-1
    },
    "method": [
      {
        "name": "novelty_guided_es",
        "behavior_traj_length": 50,
        "behavior_dim": 4,
        "adaptive": [
          {
            "initial_rl_weight": 0.0,
            "adapt": false
          }
        ],
        "novelty_detector": [
          {
            "name": "knn",
            "n_neighbors": 10,
            "archive_size": 1000
          },
          {
            "name": "ae",
            "n_hidden": ["50", "100", "100,50"],
            "lr": 1e-3,
            "batch_size": 100,
            "sparsity_level": [
              0.25, 0.5, 1.0
            ],
            "archive_size": 1000,
            "n_epochs": [5]
          }
        ]
      }
    ]
  }
  