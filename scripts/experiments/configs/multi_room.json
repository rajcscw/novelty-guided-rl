{
  "environment": {
    "domain_name": "gym",
    "name": "MiniGrid-MultiRoom-N2-S4-v0",
    "max_episode_steps": 300
  },
  "run": {
    "parallel_eval": 10,
    "runs": 3,
    "iterations": 500 ,
    "average_every": 50,
    "reset_every": 1,
    "save_every": 200,
    "initial_pad_value": 0
  },
  "model": {
    "hidden_size": 64
  },
  "ES": {
    "a": "1e-2",
    "sigma": "1e-1"
  },
  "novelty": {
    "initial_rl_weight": 1.0,
    "rl_weight_delta": 0.01,
    "t_max": 100,
    "technique": "AEnovelty",
    "adaptive": true
  },
  "behavior": {
    "traj_length": 50
  },
  "KNNnovelty": {
    "n_neighbors": 10,
    "archive_size": 1000
  },
  "AEnovelty": {
    "n_hidden": 100,
    "lr": 1e-2,
    "batch_size": 20
  },
  "PPO": {
    "nbatch_train": 50,
    "log_every": 50,
    "ent_coef": 0.0,
    "lr": 0.001,
    "vf_coef": 0.5,
    "max_grad_norm": 0.5,
    "gamma": 0.99,
    "lam": 0.95,
    "noptepochs": 1,
    "cliprange": 0.2,
    "value_network": "copy"
  }
}