{
  "environment": {
    "name": "InvertedDoublePendulum-v2",
    "max_episode_steps": null,
    "stochastic": false,
    "early_stop_reward": 10000
  },
  "model": {
    "hidden_size": 64
  },
  "ES": {
    "lr": 1e-3,
    "sigma": 1e-2
  },
  "method": [
    {
      "name": "novelty_guided_es",
      "behavior_sample_ratio": [100],
      "behavior_traj_length": [100],
      "behavior_variable": [false, true],
      "behavior_dim": 6,
      "adaptive": [
        {
          "initial_rl_weight": 1.0,
          "adapt": true,
          "rl_weight_delta": [0.05],
          "t_max": [50]
        }
      ],
      "novelty_detector": [
        {
          "name": "seq_ae",
          "n_hidden": [50],
          "n_layers": [1, 2],
          "lr": 1e-3,
          "batch_size": 100,
          "sparsity_level": [
            0.25, 0.5, 1.0
          ],
          "archive_size": 1000,
          "n_epochs": [1]
        }
      ]
    }
  ]
}
