{
    "environment": {
        "name": "InvertedPendulum-v2",
        "max_episode_steps": 10000,
        "early_stop_reward": 10000,
        "stochatic": false
    },
    "model": {
      "hidden_size": 64
    },
    "ES": {
      "lr": 1e-2,
      "sigma": 1e-1
    },
    "method": [
      {
        "name": "novelty_guided_es",
        "behavior_traj_length": 50,
        "behavior_sampling_rate": [1, 100, 200],
        "behavior_variable": [true],
        "behavior_dim": 4,
        "adaptive": [
          {
            "initial_rl_weight": 1.0,
            "adapt": true,
            "rl_weight_delta": [0.05],
            "t_max": [50]
          },
          {
            "initial_rl_weight": 0.0,
            "adapt": false
          }
        ],
        "novelty_detector": [
          {
            "name": "seq_ae",
            "n_hidden": [50],
            "n_layers": [2],
            "lr": 1e-3,
            "batch_size": 100,
            "sparsity_level": [
              0.25, 0.5, 1.0
            ],
            "archive_size": 1000,
            "n_epochs": [1]
          }
        ]
      }
    ]
  }
  