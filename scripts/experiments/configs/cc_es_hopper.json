{
  "environment": {
    "name": "Hopper-v2",
    "max_episode_steps": null
  },
  "model": {
    "hidden_size": 64
  },
  "ES": {
    "lr": 1e-2,
    "sigma": 1e-1
  },
  "method": [
    {
      "name": "rl_es",
      "novelty_detector":
      {
        "name": "none"
      },
      "adaptive": {
        "initial_rl_weight": 1.0,
        "adapt": false
      }
    },
    {
      "name": "novelty_guided_es",
      "behavior_traj_length": 50,
      "behavior_dim": 2,
      "adaptive": [
        {
          "initial_rl_weight": 1.0,
          "adapt": true,
          "rl_weight_delta": [0.02, 0.05],
          "t_max": [50]
        },
        {
          "initial_rl_weight": 0.0,
          "adapt": false
        }
      ],
      "novelty_detector": [
        {
          "name": "knn",
          "n_neighbors": 10,
          "archive_size": 1000
        },
        {
          "name": "ae",
          "n_hidden": [50],
          "lr": 1e-3,
          "batch_size": 100,
          "sparsity_level": [
            0.25, 0.5, 1.0
          ],
          "archive_size": 1000,
          "n_epochs": [1, 5]
        }
      ]
    }
  ]
}
